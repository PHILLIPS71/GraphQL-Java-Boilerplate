type User {
    id: ID!
    username: String!
    email: String!
    password: String!
    avatar: String
    createdAt: String!
    modifiedAt: String!
}

type Query {
    users: [User]
    get(id: String): User
}

type Mutation {
    create(username: String!, email: String!, password: String!): User!
    delete(id: String!): User!
}
